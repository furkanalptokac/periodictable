{
  "name": "periodictable",
  "version": "0.0.1",
  "description": "Periodic table visualization.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make-mac": "electron-forge make --platform darwin",
    "make-linux": "electron-forge make --platform linux",
    "make-windows": "electron-forge make --platform win32",
    "make-all": "yarn make-mac && yarn make-linux && yarn make-windows",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furkanalptokac/periodictable.git"
  },
